{
  "type": "module",
  "scripts": {
    "save": "git add . && git commit -m \"patch\" && git push",
    "start": "run-p start:hmr",
    "start:hmr": "node --loader ts-node/esm ./hmr/hmr-server.ts --dir \"./src\"",
    "build": "run-s build:hmr build:bundle",
    "build:bundle": "node --loader ts-node/esm ./runBundleScript.ts",
    "build:js": "rm -rf assets/js && mkdir assets/js && tsc -p tsconfig.build.json",
    "build:hmr": "node hmr/runBundleScript.hmr.mjs",
    "build:test": "node --loader ts-node/esm ./src/testbuild.ts -p tsconfig.build.json",
    "test": "jest"
  },
  "devDependencies": {
    "express": "^4.18.2",
    "npm-run-all": "^4.1.5",
    "serve-static": "^1.15.0",
    "taggedjs-cli": "^0.1.13",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "ws": "^8.15.1"
  },
  "dependencies": {
    "taggedjs": "^2.8.17",
    "taggedjs-gateway": "^1.1.1"
  }
}
